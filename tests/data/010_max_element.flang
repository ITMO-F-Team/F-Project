func maxElement (lst)
  (cond (or (not (islist lst)) (isnull lst))
    null
    (setq maxRest (maxElement (tail lst))
      (cond (or (isnull maxRest) (greatereq (head lst) maxRest))
        (head lst)
        maxRest
      )
    )
  )
)


(assert (equal (max_element '()) null))
(assert (equal (max_element '(1)) 1))
(assert (equal (max_element '(1 2 3 4 5)) 5))
(assert (equal (max_element '(1 2 5 3 4)) 5))
(assert (equal (max_element '(5 1 2 3 4)) 5))
